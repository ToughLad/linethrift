<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <layer-list>
            <item>
                <shape android:shape="oval">
                    <solid android:color="@color/linegreen" />
                </shape>
            </item>
            <item android:gravity="center" android:drawable="@drawable/chat_skin_settings_tick_icon" />
        </layer-list>
    </item>
    <item android:state_selected="false" android:drawable="@drawable/transparent" />
</selector>
