<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <layer-list>
            <item android:gravity="center" android:height="@dimen/line_user_setting_checkmark_size" android:width="@dimen/line_user_setting_checkmark_size" android:drawable="@drawable/checkbox_selected_check" />
        </layer-list>
    </item>
    <item android:state_checked="false">
        <layer-list>
            <item android:height="@dimen/line_user_setting_checkmark_size" android:width="@dimen/line_user_setting_checkmark_size" android:drawable="@drawable/checkbox_unselected_default" />
        </layer-list>
    </item>
</selector>
