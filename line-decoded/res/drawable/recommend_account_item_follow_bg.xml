<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/primaryFill" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/primaryAltSurface" />
            <stroke android:width="1.0dip" android:color="@color/primaryNeutralFill" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
</selector>
