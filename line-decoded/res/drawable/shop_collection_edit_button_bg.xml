<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/primarySubSurface" />
            <stroke android:width="1.0dip" android:color="@color/primarySubNeutralFill_pressed" />
            <corners android:radius="3.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/primarySubSurface" />
            <stroke android:width="1.0dip" android:color="@color/primarySubNeutralFill" />
            <corners android:radius="3.0dip" />
        </shape>
    </item>
</selector>
