<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/linewhite_pressed" />
            <stroke android:width="1.0dip" android:color="@color/linegray200_pressed" />
            <corners android:radius="@dimen/chat_skin_preview_button_corner_radius" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/defaultSub2NeutralFill" />
            <stroke android:width="1.0dip" android:color="@color/primaryNeutralFill" />
            <corners android:radius="@dimen/chat_skin_preview_button_corner_radius" />
        </shape>
    </item>
</selector>
