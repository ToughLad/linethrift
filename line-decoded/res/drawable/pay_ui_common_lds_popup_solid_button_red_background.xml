<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/pay_gray2" />
            <corners android:radius="@dimen/lds_popup_button_corner_radius" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/linered600_pressed" />
            <corners android:radius="@dimen/lds_popup_button_corner_radius" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/linered600" />
            <corners android:radius="@dimen/lds_popup_button_corner_radius" />
        </shape>
    </item>
</selector>
