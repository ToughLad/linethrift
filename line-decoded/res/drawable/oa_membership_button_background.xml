<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/linegray250" />
            <corners android:radius="@dimen/lds_box_button_background_corner_radius" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/primaryFill_pressed" />
            <corners android:radius="@dimen/lds_box_button_background_corner_radius" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/primaryFill" />
            <corners android:radius="@dimen/lds_box_button_background_corner_radius" />
        </shape>
    </item>
</selector>
