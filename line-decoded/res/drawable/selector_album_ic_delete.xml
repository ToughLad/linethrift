<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/album_ic_delete_pressed" android:tint="@color/senarySub2NeutralFill" />
    </item>
    <item android:state_enabled="false">
        <bitmap android:src="@drawable/album_ic_delete_pressed" android:tint="@color/senarySub2NeutralFill" />
    </item>
    <item>
        <bitmap android:src="@drawable/album_ic_delete_normal" android:tint="@color/secondaryInverseFill" />
    </item>
</selector>
