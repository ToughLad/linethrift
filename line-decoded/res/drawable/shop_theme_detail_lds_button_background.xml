<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/linewhite" />
            <stroke android:width="@dimen/lds_box_button_background_stroke_width" android:color="@color/linegray200" />
            <corners android:radius="@dimen/lds_box_button_background_corner_radius" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/linegray150" />
            <stroke android:width="@dimen/lds_box_button_background_stroke_width" android:color="@color/linegreen_pressed" />
            <corners android:radius="@dimen/lds_box_button_background_corner_radius" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/linewhite" />
            <stroke android:width="@dimen/lds_box_button_background_stroke_width" android:color="@color/linegreen" />
            <corners android:radius="@dimen/lds_box_button_background_corner_radius" />
        </shape>
    </item>
</selector>
