<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="@color/linegray250_pressed" />
            <corners android:radius="@dimen/lds_popup_button_corner_radius" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="@color/linegray250" />
            <corners android:radius="@dimen/lds_popup_button_corner_radius" />
        </shape>
    </item>
</selector>
