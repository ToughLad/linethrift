<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="7.0dip" />
            <stroke android:width="1.0dip" android:color="@color/linegreen" />
            <solid android:color="@color/userprofile_ai_avatar_gender_item_surface" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <corners android:radius="7.0dip" />
            <stroke android:width="1.0dip" android:color="@color/linegreen" />
            <solid android:color="@color/userprofile_ai_avatar_gender_item_surface" />
        </shape>
    </item>
    <item android:state_activated="false">
        <shape android:shape="rectangle">
            <corners android:radius="7.0dip" />
            <stroke android:width="1.0dip" android:color="@color/userprofile_ai_avatar_gender_item_stroke_inactivate" />
            <solid android:color="@color/userprofile_ai_avatar_gender_item_surface_inactivate" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="7.0dip" />
            <stroke android:width="1.0dip" android:color="@color/userprofile_ai_avatar_gender_item_stroke" />
            <solid android:color="@color/userprofile_ai_avatar_gender_item_surface" />
        </shape>
    </item>
</selector>
