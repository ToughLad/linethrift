<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1.0dip" android:color="@color/primarySurfaceSubPressed" />
            <corners android:radius="5.0dip" />
            <solid android:color="@color/primarySurfacePressed" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1.0dip" android:color="@color/quinaryAltFill" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
</selector>
