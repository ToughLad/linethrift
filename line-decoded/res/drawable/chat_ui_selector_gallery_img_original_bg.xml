<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="@color/tertiarySub4Fill" />
            <solid android:color="@color/transparent" />
            <corners android:radius="15.0dip" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="@color/linegreen" />
            <solid android:color="@color/transparent" />
            <corners android:radius="15.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="@color/tertiaryNeutralFill" />
            <solid android:color="@color/transparent" />
            <corners android:radius="15.0dip" />
        </shape>
    </item>
</selector>
